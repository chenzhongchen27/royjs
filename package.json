{
  "name": "@alife/roy",
  "version": "0.1.6",
  "description": "A tiny mvvm library for react",
  "main": "lib/index.js",
  "scripts": {
    "build": "rimraf lib && babel src --out-dir lib",
    "build:umd": "webpack --env dev && webpack --env build",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "mocha --require babel-core/register --colors ./test/*.spec.js",
    "test:watch": "mocha --require babel-core/register --colors -w ./test/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "http://gitlab.alibaba-inc.com/xing.gex/roy.git"
  },
  "files": [
    "dist",
    "docs",
    "lib",
    "src",
    "test",
    "HISTORY.md",
    "README.md"
  ],
  "keywords": [
    "mvvm",
    "react",
    "tiny",
    "library",
    "universal",
    "umd",
    "commonjs"
  ],
  "author": "xing.gex",
  "license": "MIT",
  "bugs": {
    "url": "http://gitlab.alibaba-inc.com/xing.gex/roy/issues"
  },
  "homepage": "http://gitlab.alibaba-inc.com/xing.gex/roy",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "eslint": "^4.13.1",
    "eslint-loader": "^1.9.0",
    "mocha": "^4.0.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "rimraf": "^2.6.2",
    "webpack": "^3.10.0",
    "yargs": "^10.0.3"
  },
  "dependencies": {
    "@alife/s-request": "^1.x"
  },
  "publishConfig": {
    "registry": "http://registry.npm.alibaba-inc.com"
  }
}
